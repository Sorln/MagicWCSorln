local Tab = Window:NewTab("Money")
local Section = Tab:NewSection("Get Rich :D")
Section:NewButton("Get $$$$", "ButtonInfo", function()
    print("Clicked")
    	local ohNumber1 = 999999999

	game:GetService("ReplicatedStorage").Remotes.GiveCurrency:FireServer(ohNumber1)
end)
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Improvments to Yourself")
Section:NewSlider("WalkSpeed", "Change WalkSpeed", 300, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Section:NewSlider("JumpPower", "Change JumpPower", 250, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
Section:NewButton("Burn Your Angel wings (look at ...)", "First become a angel then show ur wings then burn", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").BurnAngelWing:FireServer()
end)
Section:NewButton("Get Horns", "ButtonInfo", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").SpiritFused:FireServer()
end)
Section:NewButton("Sign Book Of The Beast for more Magic", "ButtonInfo", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").HaveSigned:FireServer()
end)
Section:NewButton("Beam", "ButtonInfo", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").TeleportBeam1:FireServer()
end)
Section:NewButton("Cursed Lightning", "ButtonInfo", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").Lightning:FireServer()
end)
local Tab = Window:NewTab("World")
local Section = Tab:NewSection("Change Things about the world around you")
Section:NewButton("Change Moon Phases (only works at night)", "ButtonInfo", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").MoonChange:FireServer()
end)
local Tab = Window:NewTab("TP")
local Section = Tab:NewSection("Teleport")
Section:NewButton("Go to Heaven", "ButtonInfo", function()
