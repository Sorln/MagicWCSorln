local Tab = Window:NewTab("Shop")
local Section = Tab:NewSection("Buy stuff with your money <3 ")
Section:NewButton("Buy Sealing Amber", "ButtonInfo", function()
    print("Clicked")
    local args = {
    [1] = "Sealing Amber"
}

game:GetService("ReplicatedStorage").Remotes.BuyItem:FireServer(unpack(args))
end)
