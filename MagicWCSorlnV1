end)
local Tab = Window:NewTab("Items")
local Section = Tab:NewSection("Get Good Items")
Section:NewButton("Knife", "ButtonInfo", function()
    print("Clicked")
    local player = game.Players.LocalPlayer

local rl = game.ReplicatedStorage.Knife:Clone()
rl.Parent = player.Backpack
end)
Section:NewButton("Vervain", "ButtonInfo", function()
    print("Clicked")
    local player = game.Players.LocalPlayer

local rl = game.ReplicatedStorage.Vervain:Clone()
