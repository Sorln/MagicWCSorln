local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Magic V.2 Witchcraft Script      (Sorln#3037)", "DarkTheme")
local Tab = Window:NewTab("Species")
local Section = Tab:NewSection("Change ur Species to what you want to be <3")
Section:NewButton("Mortal", "ButtonInfo", function()
    print("Clicked")
    	local ohBrickColor1 = BrickColor.new("Fossil")

	game:GetService("ReplicatedStorage").Remotes.ChangeTeam:FireServer(ohBrickColor1)
end)
Section:NewButton("Pagon Witch", "ButtonInfo", function()
    print("Clicked")
    	local ohBrickColor1 = BrickColor.new("Bright bluish green")

	game:GetService("ReplicatedStorage").Remotes.ChangeTeam:FireServer(ohBrickColor1)
end)
Section:NewButton("Fairy", "ButtonInfo", function()
    print("Clicked")
    	local ohBrickColor1 = BrickColor.new("Sea green")

	game:GetService("ReplicatedStorage").Remotes.ChangeTeam:FireServer(ohBrickColor1)
end)
